  "scripts": {
    "start": "concurrently -k --raw \"yarn dev:client\" \"yarn dev:server\"",
    "build": "yarn build:client && yarn build:server",
    "dev:client": "cross-env NODE_ENV=development webpack-dev-server --config scripts/webpack.dev.js -w --hot",
    "dev:server": "cross-env NODE_ENV=development nodemon -w server/**/* -e ts,tsx,yaml --exec 'ts-node ./server/server.ts'",
    "build:client": "webpack -p --config scripts/webpack.prod.js --display errors-only",
    "build:server": "webpack -p --config scripts/webpack.server.js --display errors-only",
    "lint": "eslint --ext .tsx,.ts --fix ./src",
    "postinstall": "rimraf .cache-loader",
    "prebuild": "rimraf dist",
    "serve": "cross-env NODE_ENV=production node ./dist/server.js",
    "dts": "dts-gen -m",
    "test": "jest"
  },
